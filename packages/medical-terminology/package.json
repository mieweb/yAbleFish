{
  "name": "@yabelfish/medical-terminology",
  "version": "0.1.0",
  "description": "Shared medical terminology database for yAbleFish",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
